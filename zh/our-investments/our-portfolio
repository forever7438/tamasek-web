<script>
    function highlightsDatatable() {
        return new DataTable('#dtMajorInvestments', {
            paging: false,
            dom: "lrtip",
            bInfo: false,
            scrollX: true,
            ordering: false,
            fixedColumns: true,
        });
    }
    setTimeout(function () {

        //highlightsDatatable()
    }, 1500);
</script>
<script>
    $(document).ready(function () {
        var dtMajorInvestmen;
        var isMobileView = window.innerWidth <= 768;

        $(".chart-footnote").on("click", "p", function (event) {
            var innerContent = $(event.currentTarget).find("sup").html();
            var targetSup = $(".table-container sup:contains(" + innerContent + ")");
            if (targetSup.length) {
                $("html, body").animate({
                    scrollTop: targetSup.offset().top - 140
                }, 500);
            }
        })
        function loadMajorInvestmentsDatatable() {
            var deviceMedia = window.matchMedia("screen and (max-width: 767px)");
            // var isMobile = window.innerWidth <= 768;


            if ($.fn.DataTable.isDataTable('#dtMajorInvestments')) {
                dtMajorInvestment.destroy();
            }


            dtMajorInvestment = new DataTable('#dtMajorInvestments', {
                paging: false,
                dom: "lrtip",
                bInfo: false,
                columns: [
                    { orderable: false },
                    null,
                    { orderable: false },
                    null,
                    { width: 185 },
                    null],
                order: [[1, '']],
                scrollX: true,
                fixedColumns: true,
                // fixedHeader: isMobile,
                responsive: true,
                fixedHeader: deviceMedia.matches,
                "drawCallback": function (settings) {

                    // This time, we target the entire wrapper, not just the table node.
                    // This is crucial for finding the cloned headers.
                    var $tableWrapper = $(this.api().table().container());

                    $tableWrapper.find('table').css('white-space', 'nowrap');

                    // We get the column count from the original, visible header.
                    var numColumns = $(this.api().table().header()).find('tr:first th').length;

                    for (var i = 1; i <= numColumns; i++) {
                        var maxWidth = 0;

                        // KEY CHANGE: Find all matching cells ANYWHERE in the wrapper.
                        // This selects the original TDs, the original THs, AND the cloned THs.
                        var $columnCells = $tableWrapper.find('th:nth-child(' + i + '), td:nth-child(' + i + ')');

                        // Before measuring, remove any pre-existing width styles to get the true content width.
                        $columnCells.css('width', '');

                        // Find the maximum width among all cells in this column (originals and clones)
                        $columnCells.each(function () {
                            var cellWidth = $(this)[0].getBoundingClientRect().width;
                            if (cellWidth > maxWidth) {
                                maxWidth = cellWidth;
                            }
                        });

                        // Apply the final calculated width to EVERY cell in the collection.
                        var finalWidth = Math.ceil(maxWidth);
                        $columnCells.css({
                            'width': finalWidth + 'px',
                            'min-width': finalWidth + 'px' // Set min-width for robustness
                        });
                    }

                    // Restore normal text wrapping now that widths are set
                    $tableWrapper.find('table').css('white-space', 'normal');

                    // Tell DataTables and FixedColumns to re-adjust to our new widths
                    this.api().columns.adjust();
                }
            });

        }

        loadMajorInvestmentsDatatable();
        function setTableColWidth() {
            var $tableWrapper = $("#dtMajorInvestments_wrapper");

            if ($tableWrapper.length > 0 && $tableWrapper.is(":visible")) {

                // 2. Select the visible header and body tables.
                var $headTable = $tableWrapper.find('.dataTables_scrollHead table');
                var $bodyTable = $tableWrapper.find('.dataTables_scrollBody table');

                // 3. Reset styles to allow natural width calculation.
                //    (Also fixed a small bug: $cols.css("") is invalid)
                $headTable.add($bodyTable).css('white-space', 'nowrap');
                $headTable.add($bodyTable).find('th, td').removeAttr('style');

                // 4. Get the number of columns from the VISIBLE header.
                var numColumns = $headTable.find('thead tr:first th').length || 0;

                // 5. Loop through each column index.
                for (var i = 1; i <= numColumns; i++) {
                    var maxWidth = 0;

                    // 6. For the current column 'i', select BOTH the header cell AND all body cells.
                    //    This is the key fix: we combine them into one collection.
                    var $columnCells = $headTable.find('th:nth-child(' + i + ')')
                        .add($bodyTable.find('td:nth-child(' + i + ')'));

                    // Filter out cells that are part of a colspan (your existing logic, which is good)
                    $columnCells = $columnCells.filter(function () {
                        return $(this).attr('colspan') === undefined;
                    });

                    // 7. Calculate the max width from this COMBINED collection.
                    $columnCells.each(function () {
                        var cellWidth = $(this)[0].getBoundingClientRect().width;
                        if (cellWidth > maxWidth) {
                            maxWidth = cellWidth;
                        }
                    });

                    // 8. Apply the single calculated maxWidth to ALL cells in the column (th and td).
                    // Use ceil to avoid sub-pixel issues that can cause wrapping.
                    var finalWidth = Math.ceil(maxWidth);
                    $columnCells.css({
                        'width': finalWidth + 'px',
                        'min-width': finalWidth + 'px'
                    });
                }

                // 9. Restore normal wrapping and table layout.
                $headTable.add($bodyTable).css('white-space', 'normal');
                $tableWrapper.find('table').css('width', '100%');
            }
        }
        setTimeout(function () {

            //setTableColWidth();
        }, 1500);
        $(window).on('resize', function () {
            clearTimeout(resizeTimer);
            resizeTimer = setTimeout(function () {
                // setTableColWidth();
            }, 250);
        });


        var resizeTimer;
        $(window).on('resize', function () {
            clearTimeout(resizeTimer);
            resizeTimer = setTimeout(function () {
                var newIsMobile = window.innerWidth <= 768;
                if (newIsMobile !== isMobileView) {
                    isMobileView = newIsMobile;
                    loadMajorInvestmentsDatatable();
                }
            }, 300);
        });


    });


</script>
</div>
</div></div><div style="clear:both"></div>
</div>

	
	
